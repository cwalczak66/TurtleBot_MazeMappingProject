<launch>
  <include file= "$(find turtlebot3_bringup)/launch/turtle3_remote.launch">
  <arg name="model" value="burger"/>
  </include>
  <node name= "rviz" pkg="rviz" type="rviz"  
     args="-d $(find lab4)/rviz/rbe3002_lab4.rviz"/> 
  <node name="map_server" pkg="map_server" type="map_server" args="$(find lab4)/maps/phase1_mapsaverTest.yaml"/> 
  <node pkg="amcl" type="amcl" name="amcl">
    <param name="min_particles" value="100"/>

    <param name="max_particles" value="3000"/>
    <param name="update_min_a" value=".02"/>
    <param name="update_min_d" value=".05"/>
    <param name="resample_interval" value="1"/>
    <param name="laser_likelihood_max_dist" value="2.0"/>

    <param name="odom_alpha1" value=".3"/>
    <param name="odom_alpha2" value=".3"/>
    <param name="odom_alpha3" value=".3"/>
    <param name="odom_alpha4" value=".3"/>
    <param name="odom_alpha5" value=".3"/>
    <param name="laser_model_type" value="beam"/>

  </node>
 
</launch> 



